<div class='container'>
    <div class='row'>
        <table class='table'>
            <thead>
                <tr>
                    <th scope='col'>#</th>
                    <th scope='col'>Date</th>
                    <th scope='col'>Title</th>
                    <th scope='col'>Subtitle</th>
                    <th scope='col'>Description</th>
                </tr>
            </thead>
            <tbody>
                {{#each updates}}
                <tr>
                    <th scope="row">..</th>
                    <td>{{createdAt}}</td>
                    <td>{{title}}</td>
                    <td>{{subtitle}}</td>
                    <td>{{description}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>